datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider = "prisma-client-js"
}

model user {
    id       Int    @id @default(autoincrement())
    username String @unique
    email    String @unique
    password String
    image    Bytes? @db.Blob
}
